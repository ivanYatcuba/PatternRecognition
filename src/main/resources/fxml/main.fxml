<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<TabPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.impl.MainController" fx:id="mainPane">
   <tabs>
      <Tab closable="false" text="Data">
         <content>
             <fx:include fx:id="dataInput" source="dataImport.fxml"/>
         </content>
      </Tab>
      <Tab closable="false" text="Recognition">
        <content>
            <fx:include fx:id="recognition" source="recognition.fxml"/>
        </content>
      </Tab>
      <Tab closable="false" text="Statistic">
        <content>
            <fx:include fx:id="statistic" source="statistic.fxml"/>
        </content>
      </Tab>
   </tabs>
</TabPane>
